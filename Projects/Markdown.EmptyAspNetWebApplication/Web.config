-->
<configuration>

    <appSettings>
        
        <!-- MarkdownVirtualPathFactory required webPages to be enabled. -->
        <add key="webPages:Enabled" value="true"/>
        
        <!-- MarkdownVirtualPathFactory required webPages version to be defined. -->
        <add key="webPages:Version" value="2.0"/>
    
    </appSettings>

    <system.web>

        <!-- Installed by Empty ASP.NET Web Application template -->
        <compilation debug="true" targetFramework="4.5" />

        <!-- Installed by Empty ASP.NET Web Application template -->
        <httpRuntime targetFramework="4.5" />

    </system.web>

    <system.webServer>

        <!-- 
            MarkdownVirtualPathFactory requires <defaultDocument />.
        
            See "Configuring a Default File" at http://msdn.microsoft.com/en-us/library/bb763179(v=vs.100).aspx for more details.
        -->
        <defaultDocument>
            <files>
                <add value="index.md" />
            </files>
        </defaultDocument>

    </system.webServer>

</configuration>